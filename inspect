#/bin/bash

echo "CONGRUENCES" > aaaa
echo "===========" >> aaaa
echo >> aaaa
./nth $1 $2 | ./find_congruences.py >> aaaa

echo "CYCLES" > bbbb
echo "======" >> bbbb
echo >> bbbb
./nth $1 $2 | ./cycles.py >> bbbb

./nth $1 $2 | interpformat tabular | grep "^ " > cccc

pr -w 110 -tFm aaaa bbbb cccc
rm aaaa bbbb cccc
